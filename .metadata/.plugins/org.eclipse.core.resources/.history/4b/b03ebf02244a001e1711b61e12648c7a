package com.msb.lrg.ds.tree;

public class TreeTraversal {

	public static void main(String[] args) {

		TreeTraversal.printTree(null);
	}

	
	public static void printTree(int[] tree) {
		int[] data = new int[16];
		for (int i = 15; i < 15+data.length; i++) {
			data[i] = i;
		}
		int k = 0;
		int p = -100;
		for (int i = 0; i < Math.pow(2, 4) - 1; i++) {
			System.out.printf("%"+i+"s", data[k++]);
			System.out.printf("%"+(i+3)+"s", data[k++]);
			p+=6;
		}
	}
	
}

