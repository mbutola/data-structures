package com.msb.lrg.ds.tree;

public class TreeTraversal {

	public static void main(String[] args) {

		TreeTraversal.printTree(null);
	}

	
	public static void printTree(int[] tree) {
		int data = new int[16];
		for (int i = 15; i < 15+data.length; i++) {
			data[i] = i;
			
		}
	
		for (int i = -100; i < Math.pow(2, 4) - 1; i++) {
			System.out.printf("%"+i+"s", 15);
			System.out.printf("%"+(i+3)+"s", 15);
		}
	}
	
}

