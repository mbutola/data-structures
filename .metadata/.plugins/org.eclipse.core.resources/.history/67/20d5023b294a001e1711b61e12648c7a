package com.msb.lrg.ds.tree;

public class TreeTraversal {

	public static void main(String[] args) {
        Utility.Node root = new Utility.Node(1);
        Utility.Node temp = null;
        temp = new Utility.Node(2);
        root.left = temp;
        temp = new Utility.Node(3);
        root.right = temp;
 
        temp = new Utility.Node(4);
        root.left.left = temp;
        temp = new Utility.Node(5);
        root.left.right = temp;
        temp = new Utility.Node(6);
        root.right.left = temp;
        temp = new Utility.Node(7);
        root.right.right = temp;
 
        temp = new Utility.Node(8);
        root.left.left.left = temp;
        temp = new Utility.Node(9);
        root.left.left.right = temp;
        temp = new Utility.Node(10);
        root.left.right.left = temp;
        temp = new Utility.Node(11);
        root.left.right.right = temp;
        temp = new Utility.Node(12);
        root.right.left.left = temp;
        temp = new Utility.Node(13);
        root.right.left.right = temp;
        temp = new Utility.Node(14);
        root.right.right.left = temp;
        temp = new Utility.Node(15);
        root.right.right.right = temp;
 
        printBinaryTree(root);	}

}

