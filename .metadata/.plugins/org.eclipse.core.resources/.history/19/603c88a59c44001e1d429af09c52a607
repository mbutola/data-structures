package com.msb.lrg.ds.dp;

public class CountBSTDp {

	public static void main(String[] args) {
		int nodes = 5;
		System.out.println("O/P : " + CountBSTDp.count(nodes));
	}

	public static int count(int n) {
		
		int[] dp = new int[n+1]; 
		dp[0] = 1;
		
		for (int i = 1; i <= n ; i++) {
			dp[i] = 0;
			for (int j = 0; j < i; j++) {
				dp[i] = dp[i] + dp[i]*dp[j-1];
			}
			
		}
				
		return dp[n];
	}
}
