package com.msb.lrg.c;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class CollectionRunner {

	public static void main(String[] args) {
		List<String> names = new ArrayList<>(Arrays.asList("Ranjan", "Manmohan", "Mithun", "Subodh"));
		System.out.println("names: " + names);
		names.sort(new Comparator<String>() {
			@Override
			public int compare(String o1, String o2) {
				// TODO Auto-generated method stub
				return o1.compareTo(o2);
			}
		});
		System.out.println("names: " + names);

		List<String> names1 = new ArrayList<>(Arrays.asList("Ranjan", "Manmohan", "Mithun", "Subodh"));
		List<String> names2 = names1.stream().sorted(Comparator.reverseOrder()).collect(Collectors.toList());
		System.out.println("names1: " + names1);
		System.out.println("names2: " + names2);
		
		List<String> names3 = new ArrayList<>(Arrays.asList("Ranjan", "Manmohan", "Mithun", "Subodh"));
		Comparator<String> comp = (o1,o2) -> o1.compareTo(o2);
		names3.sort(comp);
		System.out.println("names3: " + names3);
		
		List<String> names4 = new ArrayList<>(Arrays.asList("Ranjan", "Manmohan", "Mithun", "Subodh"));
		Comparator<String> compMethodRef = (o1,o2) -> o1.compareTo(o2);
		names4.sort(compMethodRef::compare);
		System.out.println("names4: " + names4);
	}

}
