package com.msb.lrg.ds.tree;

public class TreeTraversal {

	public static void main(String[] args) {

		TreeTraversal.printTree(null);
	}

	
	public static void printTree(int[] tree) {
		int[] data = new int[16];
		for (int i = 0; i < data.length; i++) {
			data[i] = i+15;
		}
		int k = 0;
		int p = 0;
		for (int i = 0; i < Math.pow(2, 3); i++) {
			System.out.printf("%"+p+"s", data[k++]);
			System.out.printf("%"+(p+3)+"s", data[k++]);
			p+=6;
		}
	}
	
}

