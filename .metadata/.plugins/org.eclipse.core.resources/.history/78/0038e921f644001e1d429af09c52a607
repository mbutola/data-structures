package com.msb.lrg.ds.dp;

public class MtxMultDP {

	public static void main(String[] args) {
		int[] matrixes = new int[] {20,10,30,40};
		System.out.println("O/P : " + MtxMultResursive.MtxMultDP(matrixes, 0, matrixes.length-1));
	}

	public static int multiplications(int[] mat, int i, int j) {
		
		if(i+1==j)
			return 0;
		
		int res = Integer.MAX_VALUE;
		
		for (int k = i+1; k < j; k++) {
			res = Math.min(res, multiplications(mat, i, k)
									+ multiplications(mat, k, j)
									+ mat[i]*mat[k]*mat[j]);
		}
		
		return res;
	}
}
