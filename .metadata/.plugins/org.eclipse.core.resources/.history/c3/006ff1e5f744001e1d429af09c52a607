		System.out.println("O/P : " + MtxMultDP.multiplications(matrixes, matrixes.length));
	}

	public static int multiplications(int[] mat, int n) {
		
//		if(i+1==j)
//			return 0;
//		
//		int res = Integer.MAX_VALUE;
//		
//		for (int k = i+1; k < j; k++) {
