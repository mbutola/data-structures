		int[][] dp = new int[n+1][n+1];
		
		for (int i = 0; i < n; i++) {
			dp[i][i+1] = 0;
		}
		
		Utility.printDp(Utility.getList(n), Utility.getList(n), dp);
		return dp[0][n];
		
	}
